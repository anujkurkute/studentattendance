<%- include('partials/header') %> <%- include('partials/Facultynavbar') %>

<div class="container my-5 font">
  <form action="/StudentList" method="post">
    <table class="table table-dark table-striped">
      <thead>
        <tr>
          <th scope="col">ID</th>
          <th scope="col">Name</th>
          <th scope="col">Roll No</th>
          <th scope="col">Email</th>
        </tr>
      </thead>
      <tbody>
        <% studentArray.forEach(function(student){ %>
        <tr>
          <th scope="row" name="sid"><%=i%></th>
          <td name="sname"><%= student.name %></td>
          <td name="srollno"><%= student.rollno %></td>
          <td name="srollno"><%= student.email %></td>
        </tr>
        <% i++ %> <% }) %>
      </tbody>
    </table>
  </form>
</div>

<%- include('partials/footer') %>
